{
  "cdr_template": {
    "name": "webitel",
    "body": {
      "index_patterns": [
        "cdr-*"
      ],
      "order": 2,
      "mappings": {
        "cdr": {
          "dynamic_templates": [
            {
              "string_fields": {
                "match_mapping_type": "*",
                "match": "*",
                "mapping": {
                  "type": "keyword"
                }
              }
            },
            {
              "integers": {
                "match_mapping_type": "long",
                "match": "*",
                "mapping": {
                  "type": "keyword"
                }
              }
            },
            {
              "notanalyzed": {
                "match": "*",
                "mapping": {
                  "type": "keyword"
                }
              }
            }
          ],
          "properties": {
            "uuid": {
              "type": "keyword"
            },
            "parent_uuid": {
              "type": "keyword"
            },
            "leg": {
              "type": "keyword"
            },
            "pinned_items": {
              "type": "keyword"
            },
            "direction": {
              "type": "keyword"
            },
            "caller_id_name": {
              "type": "keyword"
            },
            "caller_id_number": {
              "type": "keyword"
            },
            "network_addr": {
              "type": "keyword"
            },
            "destination_number": {
              "type": "keyword"
            },
            "domain_name": {
              "type": "keyword"
            },
            "extension": {
              "type": "keyword"
            },
            "presence_id": {
              "type": "keyword"
            },
            "source": {
              "type": "keyword"
            },
            "gateway": {
              "type": "keyword"
            },
            "hangup_cause_q850": {
              "type": "long"
            },
            "hangup_cause": {
              "type": "keyword"
            },
            "hangup_disposition": {
              "type": "keyword"
            },
            "transfer_disposition": {
              "type": "keyword"
            },
            "originate_disposition": {
              "type": "keyword"
            },
            "created_time": {
              "type": "date"
            },
            "duration": {
              "type": "long"
            },
            "billsec": {
              "type": "long"
            },
            "progresssec": {
              "type": "long"
            },
            "answersec": {
              "type": "long"
            },
            "waitsec": {
              "type": "long"
            },
            "holdsec": {
              "type": "long"
            },
            "quality_percentage_audio": {
              "type": "long"
            },
            "quality_percentage_video": {
              "type": "long"
            },
            "locations": {
              "type": "object",
              "properties": {
                "geo": {
                  "type": "geo_point"
                },
                "city": {
                  "type": "keyword"
                },
                "country": {
                  "type": "keyword"
                },
                "country_code": {
                  "type": "keyword"
                },
                "type": {
                  "type": "keyword"
                }
              }
            },
            "queue": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "keyword"
                },
                "duration": {
                  "type": "long"
                },
                "wait_duration": {
                  "type": "long"
                },
                "cancel_reason": {
                  "type": "keyword"
                },
                "cause": {
                  "type": "keyword"
                },
                "answered_time": {
                  "type": "date"
                },
                "exit_time": {
                  "type": "date"
                },
                "joined_time": {
                  "type": "date"
                },                
                "side": {
                  "type": "keyword"
                }
              }
            },
            "post_data": {
              "type": "object",
              "properties": {
                "comment": {
                  "type": "keyword"
                },
                "vote": {
                  "type": "long"
                }
              }
            },
            "variables": {
              "type": "object",
              "properties": {
                "conference_member_id": {
                  "type": "long"
                },
                "sip_invite_failure_status": {
                  "type": "long"
                },
                "video_read_rate": {
                  "type": "long"
                },
                "video_write_rate": {
                  "type": "long"
                }
              }
            },
            "recordings": {
              "type": "nested",
              "include_in_parent": true,
              "properties": {
                "content-type": {
                  "type": "keyword"
                },
                "createdOn": {
                  "type": "date"
                },
                "domain": {
                  "type": "keyword"
                },
                "name": {
                  "type": "keyword"
                },
                "path": {
                  "type": "keyword"
                },
                "private": {
                  "type": "boolean",
                  "store": true
                },
                "size": {
                  "type": "long",
                  "store": true
                },
                "type": {
                  "type": "integer"
                },
                "uuid": {
                  "type": "keyword"
                }
              }
            },
            "callflow": {
              "type": "nested",
              "include_in_parent": true,
              "properties": {
                "caller_profile": {
                  "type": "object",
                  "properties": {
                    "username": {
                      "type": "keyword",
                      "store": true
                    },
                    "caller_id_name": {
                      "type": "keyword",
                      "store": true
                    },
                    "ani": {
                      "type": "keyword",
                      "store": true
                    },
                    "aniii": {
                      "type": "keyword",
                      "store": true
                    },
                    "caller_id_number": {
                      "type": "keyword",
                      "store": true
                    },
                    "network_addr": {
                      "type": "keyword",
                      "store": true
                    },
                    "rdnis": {
                      "type": "keyword",
                      "store": true
                    },
                    "destination_number": {
                      "type": "keyword",
                      "store": true
                    },
                    "uuid": {
                      "type": "keyword",
                      "store": true
                    },
                    "source": {
                      "type": "keyword",
                      "store": true
                    }
                  }
                },
                "times": {
                  "type": "object",
                  "properties": {
                    "created_time": {
                      "type": "date"
                    },
                    "profile_created_time": {
                      "type": "date"
                    },
                    "progress_time": {
                      "type": "date"
                    },
                    "progress_media_time": {
                      "type": "date"
                    },
                    "answered_time": {
                      "type": "date"
                    },
                    "bridged_time": {
                      "type": "date"
                    },
                    "last_hold_time": {
                      "type": "date"
                    },
                    "hold_accum_time": {
                      "type": "date"
                    },
                    "hangup_time": {
                      "type": "date"
                    },
                    "resurrect_time": {
                      "type": "date"
                    },
                    "transfer_time": {
                      "type": "date"
                    }
                  }
                }
              }
            }            
          }
        }
      }
    }
  },
  "accounts_template":{
    "name": "accounts",
    "body": {
      "index_patterns": [
        "accounts-*"
      ],
      "order": 3,
      "mappings": {
        "accounts": {
          "dynamic_templates": [            
            {
              "notanalyzed": {
                "match": "*",
                "mapping": {
                  "type": "keyword"
                }
              }
            }
          ],
          "properties": {
            "created_time": {
              "type": "date"
            },
            "end_time": {
              "type": "date"
            },
            "duration": {
              "type": "long"
            },            
            "domain": {
              "type": "keyword"
            },
            "account": {
              "type": "keyword"
            },
            "status": {
              "type": "keyword"
            },
            "state": {
              "type": "keyword"
            },
            "description": {
              "type": "keyword"
            },
            "display_status": {
              "type": "keyword"
            },
            "extension": {
              "type": "keyword"
            },
            "presence_id": {
              "type": "keyword"
            },
            "ws": {
              "type": "boolean"
            },
            "cc": {
              "type": "boolean"
            }
          }
        }
      }
    }
  }
}