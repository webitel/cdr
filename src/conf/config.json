{
  "isArchive": false,   
  "maxGoroutines": 25, 
  "insertGoroutines": 25,
  "pg": {
      "user": "webitel",
      "database": "webitel",
      "password": "webitel",
      "host": "postgres",
      "cdrTableA": "cdr_a",
      "cdrTableB": "cdr_b",
      "port": 5432,
      "orderBy": "asc"
  },
  "broker":{
      "publisher": {
          "enable": true,
          "connectionString": "amqp://webitel:secret@10.10.10.200:5672?heartbeat=0",
          "exchangeName": "TAP.CDR-Events",
          "exchangeType": "direct",
          "routingKeyLegA": "cdr-leg-a",
          "routingKeyLegB": "cdr-leg-b",
          "bulkCount": 100,
          "intervalMillisec": 1000
      },
      "receiver": {
        "enable": false,
        "connectionString": "amqp://webitel:secret@10.10.10.200:5672?heartbeat=0",
        "exchangeName": "TAP.CDR-Events",
        "exchangeType": "direct",
        "routingKeyLegA": "cdr-leg-a",
        "routingKeyLegB": "cdr-leg-b",
        "bulkCount": 100,
        "intervalMillisec": 1000
      },
      "account": {
          "enable": true,
          "connectionString": "amqp://webitel:secret@10.10.10.200:5672?heartbeat=0",
          "exchangeName": "Storage.Commands",
          "exchangeType": "topic",
          "routingKey": "log.user.*.*.status",          
          "bulkCount": 100,
          "intervalMillisec": 1000
      }
  },
  "elastic":{
      "enable": true,
      "httpAuth": "",
      "deleteTemplate": true,
      "host": "http://elasticsearch2:9200",
      "intervalMillisec": 1000,
      "bulkCount": 100,
      "indexNameCdr": "cdr",
      "indexNameCdrTemplate": "${INDEX}-${LEG}-${YEAR}-${MONTH}-${DOMAIN}",
      "indexNameAccounts": "accounts",
      "indexNameAccountsTemplate": "${INDEX}-${YEAR}-${MONTH}-${DOMAIN}"
  },
  "application": {
    "loglevel": "debug"
  }   
}    
